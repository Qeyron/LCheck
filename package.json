{
  "name": "nextjs-base-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",

    "lint": "oxlint",
    "type-check": "tsc --noEmit",
    "check-all": "npm run lint & npm run type-check & wait",

    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && npx wrangler pages dev",
    "deploy": "npm run pages:build && npx wrangler pages deploy"
  },
  "dependencies": {
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "js-sha3": "^0.9.3",
    "next": "14.2.23",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "oxlint": "^1.12.0",
    "postcss": "^8",
    "supabase": "2.34.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
  
}
